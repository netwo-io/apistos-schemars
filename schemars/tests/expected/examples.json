{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Struct",
  "type": "object",
  "properties": {
    "foo": {
      "type": "integer",
      "format": "int32",
      "examples": [
        8,
        null
      ]
    },
    "bar": {
      "type": "boolean"
    },
    "baz": {
      "type": [
        "string",
        "null"
      ],
      "examples": [
        null
      ]
    }
  },
  "required": [
    "foo",
    "bar"
  ],
  "examples": [
    {
      "foo": 0,
      "bar": false,
      "baz": null
    },
    null
  ]
}