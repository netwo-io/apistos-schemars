{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "MyStruct",
  "type": "object",
  "properties": {
    "myInt": {
      "type": "integer"
    },
    "myBool": {
      "type": "boolean"
    },
    "myNullableEnum": true,
    "myInnerStruct": {
      "type": "object",
      "properties": {
        "my_map": {
          "type": "object",
          "additionalProperties": {
            "type": "number"
          }
        },
        "my_vec": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "my_empty_map": {
          "type": "object",
          "additionalProperties": true
        },
        "my_empty_vec": {
          "type": "array",
          "items": true
        },
        "my_tuple": {
          "type": "array",
          "items": [
            {
              "type": "string",
              "minLength": 1,
              "maxLength": 1
            },
            {
              "type": "integer"
            }
          ],
          "maxItems": 2,
          "minItems": 2
        }
      }
    }
  },
  "examples": [
    {
      "myInt": 123,
      "myBool": true,
      "myNullableEnum": null,
      "myInnerStruct": {
        "my_map": {
          "": 0.0
        },
        "my_vec": [
          "hello",
          "world"
        ],
        "my_empty_map": {},
        "my_empty_vec": [],
        "my_tuple": [
          "ðŸ’©",
          42
        ]
      }
    }
  ]
}