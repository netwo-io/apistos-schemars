{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Process",
  "type": "object",
  "properties": {
    "command_line": {
      "type": "string"
    },
    "durations": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/Duration"
      }
    },
    "wall_time": {
      "$ref": "#/definitions/Duration"
    }
  },
  "required": [
    "command_line",
    "wall_time",
    "durations"
  ],
  "definitions": {
    "Duration": {
      "type": "object",
      "properties": {
        "nanos": {
          "type": "integer",
          "format": "int32"
        },
        "secs": {
          "type": "integer",
          "format": "int64"
        }
      },
      "required": [
        "secs",
        "nanos"
      ]
    }
  }
}
